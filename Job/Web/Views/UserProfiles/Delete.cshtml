@model Domain.Entities.UserProfile

<h2>Удаление профиля</h2>
<h3>Вы уверены, что хотите удалить этот профиль?</h3>

<form asp-action="DeleteConfirmed" asp-route-id="@Model.IdUP" method="post">
    <input type="hidden" asp-for="IdUP" />
    <button type="submit" class="btn btn-danger">Удалить</button>
    <a asp-action="Index" class="btn btn-secondary">Отмена</a>
</form>